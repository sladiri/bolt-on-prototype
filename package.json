{
  "name": "boltonprototype",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Slađan Ristić",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/sladiri/bolt-on-prototype"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node --experimental-modules ./src/server/index.mjs",
    "start:dev": "cross-env NODE_ENV=development node --inspect-brk --experimental-modules ./src/server/index.mjs",
    "test:load": "artillery run artillery.yaml -k",
    "build:sprites": "mkdirp ./build && svg-sprite-generate -d ./assets/**/*.svg -o ./build/sprites.svg",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack",
    "clean": "rimraf ./public",
    "prettier:check": "prettier -l src/**/*.js",
    "prettier:write": "prettier --write src/**/*.js",
    "test": "npm run test:base -- src/**/*.test.js | faucet",
    "test:debug": "npm run test:base -- src/**/*.test.js --inspect-brk | faucet",
    "test:base": "babel-node node_modules/.bin/tape",
    "foo": "browserify -t babelify src/bolt-on/control/is-covered.test.js | browser-run -p 2222 | faucet"
  },
  "devDependencies": {
    "artillery": "^1.6.0-17",
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "faucet": "^0.0.1",
    "jsverify": "^0.8.3",
    "koa-webpack": "^4.0.0",
    "prettier": "^1.12.1",
    "tape": "^4.9.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.47",
    "@babel/plugin-proposal-async-generator-functions": "^7.0.0-beta.47",
    "@babel/plugin-proposal-function-bind": "^7.0.0-beta.47",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.47",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "assert": "^1.4.1",
    "babel-loader": "^8.0.0-beta.3",
    "compression-webpack-plugin": "^1.1.11",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^1.1.11",
