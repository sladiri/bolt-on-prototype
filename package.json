    "mini-css-extract-plugin": "^0.4.0",
    "mkdirp": "^0.5.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.5",
    "preload-webpack-plugin": "^3.0.0-alpha.3",
    "ramda": "^0.25.0",
    "rimraf": "^2.6.2",
    "rxjs": "^6.1.0",
